import java.util.Scanner;
public class Lab06problem01
{
  public static void main(String[]args)
  {
    Scanner A = new Scanner(System.in);
    System.out.println("Why So Serious ! Let's put a smile on that face ! ! !");
    int k = A.nextInt();
    int a;
      for(a=1;a<=k;a++)
      {
        System.out.print(a);
      }
  }
}
