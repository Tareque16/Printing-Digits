import java.util.Scanner;
public class Lab06problem03
{
  public static void main(String[]args)
  {
    Scanner A = new Scanner(System.in);
    System.out.println("faizlami bondho koro ! ! !");
    int k = A.nextInt();
    int a,b;
      for(a=1;a<=k;a++)
      {
        for(b=1;b<=6;b++)
        {
          System.out.print("*");
        }
        System.out.println();
      }
  }
}
